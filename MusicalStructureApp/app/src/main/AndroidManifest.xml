<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MusicalStructureApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RockActivity"
                  android:label="@string/rock_text" />
        <activity android:name=".LoveActivity"
                  android:label="@string/love_text" />
        <activity android:name=".FilmActivity"
                  android:label="@string/film_text" />
        <activity android:name=".AfricanActivity"
                  android:label="@string/african_text" >
        </activity>
    </application>

</manifest>